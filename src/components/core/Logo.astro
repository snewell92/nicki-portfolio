---
import config from "@/config/config.json";
import { Image } from "astro:assets";
const {
  logo,
  logo_width,
  logo_height,
  title,
}: {
  logo: string;
  logo_text: string;
  logo_width: string;
  logo_height: string;
  title: string;
} = config.site;
---

<a href="/" class="mx-auto">
  <Image
    src={logo}
    alt={title}
    class="w-[500px]"
    width={Number(logo_width)}
    height={Number(logo_height)}
  />
</a>
